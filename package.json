{
  "name": "stylelint-config-primer",
  "version": "2.0.0",
  "description": "Sharable stylelint config used by GitHub's CSS",
  "main": "index.js",
  "scripts": {
    "ava": "ava --verbose \"tests/**/*.js\"",
    "lint": "eslint **/*.js",
    "test": "npm run lint && npm run ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primer/stylelint-config-primer.git"
  },
  "keywords": [
    "github",
    "primer",
    "stylelint-config",
    "stylelint",
    "css"
  ],
  "browserslist": [
    "> 5%",
    "last 2 firefox versions",
    "last 2 chrome versions",
    "last 2 safari versions",
    "last 2 edge versions",
    "ie 11"
  ],
  "author": "GitHub, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/primer/stylelint-config-primer/issues"
  },
  "homepage": "https://github.com/primer/stylelint-config-primer#readme",
  "dependencies": {
    "stylelint-no-unsupported-browser-features": "^1.0.0",
    "stylelint-order": "^0.4.4",
    "stylelint-scss": "^1.4.1",
    "stylelint-selector-no-utility": "^1.5.0"
  },
  "devDependencies": {
    "ava": "latest",
    "eslint": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-github": "latest",
    "stylelint": "^7.13.0"
  },
  "peerDependencies": {
    "stylelint": "^7.13.0"
  }
}
